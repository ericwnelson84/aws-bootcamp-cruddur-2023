#! /usr/bin/bash

ABS_PATH=$(readlink -f "0")
PARENT_PATH=$(dirname $ABS_PATH)
PHOTO_PATH=$PARENT_PATH/bin/serverless/files/data.jpg

# echo $SERVERLESS_PROJECT_PATH

aws s3 rm "$PHOTO_PATH" "s3://assets.$DOMAIN_NAME/avatars/original/data.jpg"
aws s3 rm "$PHOTO_PATH" "s3://assets.$DOMAIN_NAME/avatars/processed/data.png"